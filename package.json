{
  "name": "nstb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generator": "node scripts/i18nGenerator.js",
    "aliases": "node scripts/aliases.js",
    "config": "node scripts/config.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "package": "node scripts/package.js",
    "watch": "node scripts/watch.js",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "cover:unit": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm run test:unit",
    "report:coverage": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-extend-layout": "^1.1.3",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-unit-mocha": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-airbnb": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-plugin-istanbul": "^5.0.1",
    "beINFW": "^1.2.11",
    "chai": "^4.1.2",
    "commander": "^2.17.1",
    "copy-dir": "^0.4.0",
    "cross-env": "^5.2.0",
    "csv-parse": "^2.5.0",
    "glob": "^7.1.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdoc3-parser": "^2.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "npm-run": "^5.0.1",
    "nyc": "^13.0.1",
    "rm-dir": "^0.2.2",
    "vue-template-compiler": "^2.5.17",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/**/*.{js,vue}"
    ],
    "exclude": [
      "src/*.js",
      "tests/helpers/*.js",
      "node_modules"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "extension": [
      ".js",
      ".vue"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "all": true
  }
}
